{
  "name": "amsapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "npm run dev",
    "server": "babel-node ./src/bin/www",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/bin/www.js"
  },
  "dependencies": {
    "@types/jest": "^25.1.2",
    "@types/supertest": "^2.0.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "*",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "jest": "^25.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "multer": "^1.3.10",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.0",
    "ts-jest": "^25.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.10.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.37",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/swagger-ui-express": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
